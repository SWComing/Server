function GetDungeonFatigueType( iDifficultLevel )

	if iDifficultLevel == DIFFICULTY_LEVEL["DL_NORMAL"] or iDifficultLevel == DIFFICULTY_LEVEL["DL_HARD"] or iDifficultLevel == DIFFICULTY_LEVEL["DL_EXPERT"] or iDifficultLevel == DIFFICULTY_LEVEL["DL_TEST"] then

		return DUNGEON_FATIGUE_TYPE["DFT_TYPE_1"] --普通疲勞

	elseif iDifficultLevel == DIFFICULTY_LEVEL["DL_HELL"] then

		return DUNGEON_FATIGUE_TYPE["DFT_TYPE_2"] --煉獄疲勞

	else

		return DUNGEON_FATIGUE_TYPE["DFT_NULL"]   --iDifficultLevel參數錯誤

	end
end

